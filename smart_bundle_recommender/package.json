{
  "name": "smart-bundle-recommender",
  "private": true,
  "packageManager": "pnpm@9.15.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "pnpm -r dev",
    "build": "pnpm -r build",
    "typecheck": "pnpm -r typecheck",
    "docker:up": "docker compose up --build",
    "docker:down": "docker compose down -v"
  }
}
